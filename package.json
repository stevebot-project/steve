{
  "name": "steve",
  "version": "5.0.0",
  "description": "A multipurpose Discord bot for Nerdfighter communities.",
  "main": "dist/src/Steve.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "build-clear": "rm -rf dist && tsc -p .",
    "dev": "tsc -p . && node dist/src/Steve.js",
    "dev-clear": "rm -rf dist && tsc -p . && node dist/src/Steve.js",
    "start": "node dist/src/Steve.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevebot-project/steve.git"
  },
  "imports": {
    "#utils/*": "./dist/src/lib/util/*.js",
    "#lib/*": "./dist/src/lib/*.js",
    "#root/*": "./dist/*.js"
  },
  "author": "Jonathan Ford",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/stevebot-project/steve/issues"
  },
  "homepage": "https://github.com/stevebot-project/steve#readme",
  "dependencies": {
    "@sapphire/decorators": "^6.1.0",
    "@sapphire/duration": "^1.1.2",
    "@sapphire/framework": "^5.0.10",
    "@sapphire/plugin-i18next": "^7.1.2",
    "@sapphire/stopwatch": "^1.5.2",
    "@sapphire/timestamp": "^1.0.3",
    "@sapphire/utilities": "^3.15.3",
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@sapphire/eslint-config": "^5.0.4",
    "@sapphire/ts-config": "^5.0.1",
    "eslint": "^8.57.0",
    "typescript": "^5.4.3"
  }
}
